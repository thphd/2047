{%extends 'base.html.jinja'%}
{%block content%}

<div class="threadlist">

  {%for c in conversations%}
    {%if c and c.last and c.last.user and c.last.to_user%}
    {{macros.post_conversation(c)}}
    {%endif%}
  {%endfor%}

</div>
<div class="padlr">
  <div class="post_content warning">
    {{messaging_warning|safe}}
  </div>

</div>

{%endblock%}
